# SETUP: construct working folder layout
exec cp $REPODIR/tests/scripts/graph.sh $WORKDIR/graph.sh
cd $WORKDIR

env DUPLICATE_WRITES=examples/graph/duplicate_writes
env REPORTS=graph/cmd/graph/reports

# TEST: Duplicate Writes Report.
exec sh -e graph.sh report -tag signal -reports=$REPODIR/$REPORTS/duplicate_writes.yaml $REPODIR/$DUPLICATE_WRITES/sim_good

# TEST: Duplicate Writes Report with error.
! exec sh -e graph.sh report --reports=$REPODIR/$REPORTS/duplicate_writes.yaml $REPODIR/$DUPLICATE_WRITES/sim_with_error
