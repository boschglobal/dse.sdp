exec cp $REPODIR/tests/testscript/scripts/workflow.sh $WORK/workflow.sh
exec sh -e $WORK/workflow.sh openloop.dse

stdout 'End Time: 0.006000'
stdout 'val=11.000000, final_val=11\.000000, name=counter'

-- openloop.dse --
simulation endtime=0.006
channel signal

uses
dse.sdp https://github.com/boschglobal/dse.sdp $RELEASE_VERSION

model simple Simple
channel signal data
file signalgroup.yaml signalgroup.yaml

-- signalgroup.yaml --
---
kind: SignalGroup
metadata:
  name: data
  labels:
    model: simple
    signal: data
spec:
  signals:
    - signal: counter
